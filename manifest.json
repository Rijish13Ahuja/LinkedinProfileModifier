{
    "manifest_version": 3,
    "name": "LinkedIn Profile Picture Modifier",
    "version": "1.0",
    "description": "Modify LinkedIn profile pictures in the home feed.",
    "permissions": ["activeTab"],
    "content_scripts": [
        {
            "matches": ["*://*.linkedin.com/*"],
            "js": ["content.js"]
        }
    ],
    "icons": {
        "16": "16.png",
        "32": "48.png",
        "48": "64.png",
        "128": "128.png"
    }
}
